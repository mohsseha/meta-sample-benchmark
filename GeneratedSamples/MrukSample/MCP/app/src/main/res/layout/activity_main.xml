<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/anchor_mesh_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Anchor Mesh Demo" />

    <Button
        android:id="@+id/keyboard_tracker_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Keyboard Tracker Demo" />

    <Button
        android:id="@+id/qr_code_scanner_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QR Code Scanner Demo" />

    <Button
        android:id="@+id/raycast_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Raycast Demo" />

</LinearLayout>
